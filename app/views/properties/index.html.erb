<h1>物件一覧</h1>
  <table>
    <tr>
      <th>物件名</th>
      <th>賃料</th>
      <th>住所</th>
      <th>築年数</th>
      <th>備考</th>
    </tr>

    <% @properties.each do |property| %>
        <tr>
          <td><%= property.name %></td>
          <td><%= property.price %></td>
          <td><%= property.adress %></td>
          <td><%= property.age %></td>
          <td><%= property.content %></td>
          <td><%=link_to'詳細',property_path(property.id) %></td>
        </tr>
    <% end %>
  </table>

